    "/v2/cryptography/algorithms": {
      "post": {
        "summary": "Get Cryptographic algorithms associated with a list of PURLs and, optionally, a requirement",
        "operationId": "Cryptography_GetAlgorithms",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v2AlgorithmResponse"
            }
          },
          "404": {
            "description": "Returned when the resource does not exist.",
            "schema": {
              "type": "string",
              "format": "string"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": "Purl request data (JSON payload)\nWARNING: PurlRequest is deprecated, use ComponentsRequest instead.",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v2PurlRequest"
            }
          }
        ],
        "tags": [
          "Cryptography"
        ]
      }
    },
